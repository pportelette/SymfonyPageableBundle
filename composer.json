{
    "name": "pportelette/pageable-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle provides pagination repository's method based on Doctrine Paginator",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Pierre Portelette",
            "email": "pierre@cloudnite.net"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "doctrine/doctrine-bundle": "^2.7",
        "symfony/framework-bundle": "^5.3|^6.0|7.0.*"
    },
    "require-dev": {
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Pportelette\\PageableBundle\\": "src/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    }
}
